@model IEnumerable<EventManagerApp.Models.Ticket>
@{
    ViewBag.Title = "Index";
}

<table class="table table-bordered table-striped" style="width:100%">
    <thead>
        <tr>
            <th>
                Id
            </th>
            <th>
                Ticket owner
            </th>
            <th>
                Event name
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td style="width:20px">
                    @obj.Id
                </td">
                <td style="width:20%">
                    @obj.Customer.FirstName @obj.Customer.LastName
                </td>
                <td>
                    @*@obj.Event.Name*@
                </td>
            </tr>
        }
    </tbody>
</table>

